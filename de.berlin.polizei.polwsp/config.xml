<?xml version='1.0' encoding='utf-8'?>
<widget id="de.berlin.polizei.polwsp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bordbuch WSP</name>
    <description>
        Eine App f√ºr mobile Bootsstreifen.
    </description>
    <author email="christoph.huschenhoefer@polizei.berlin.de" href="http://www.berlin.de/polizei">
        Christoph Huschenhoefer
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="KeepRunning" value="false"/>
        <preference name="LoadUrlTimeoutValue" value="10000"/>
        <preference name="InAppBrowserStorageEnabled" value="true"/>
        <preference name="LoadingDialog" value="Testdialog"/>
        <preference name="ErrorUrl" value="myErrorPage.html"/>
        <preference name="ShowTitle" value="true"/>
        <preference name="LogLevel" value="VERBOSE"/>
        <preference name="AndroidLaunchMode" value="singleTop"/>
    </platform>
</widget>
